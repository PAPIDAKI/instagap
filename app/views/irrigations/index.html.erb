
<h1>Listing Irrigations</h1>

<table calss="'table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Duration</th>
      <th>Qunatity</th>
      <th>Operatorpersons</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @irrigations.each do |irrigation| %>
      <tr>
        <td><%= irrigation.date %></td>
        <td><%= irrigation.duration %></td>
        <td><%= irrigation.qunatity %></td>
        <td><%= irrigation.operatorpersons %></td>
        <td><%= irrigation.note %></td>
        <td><%= link_to 'Show', irrigation %></td>
        <td><%= link_to 'Edit', edit_irrigation_path(irrigation) %></td>
        <td><%= link_to 'Destroy', irrigation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Irrigation', new_irrigation_path %>
